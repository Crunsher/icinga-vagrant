{
    "name": "Nextdoor-kibana5",
    "version": "1.0.0",
    "author": "Nextdoor",
    "license": "Apache-2.0",
    "source": "https://github.com/Nextdoor/puppet-kibana5",
    "project_page": "https://github.com/Nextdoor/puppet-kibana5",
    "issues_url": "https://github.com/Nextdoor/puppet-kibana5/issues",
    "summary": "Nextdoor kibana5 Management Module",
    "tags": [ "kibana5", "elasticsearch", "kibana", "elk", "logstash" ],
    "operatingsystem_support": [
